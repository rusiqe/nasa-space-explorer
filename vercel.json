{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "devCommand": "cd frontend && npm start",
  "installCommand": "cd frontend && npm install",
  "outputDirectory": "frontend/build",
  "framework": "create-react-app",
  "public": true,
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ]
}
